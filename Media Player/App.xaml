    <Application.Resources>
        <Style TargetType="Border"
           x:Key="video_border">
            <Setter Property="Width" Value=" 200"/>
            <Setter Property="BorderThickness" Value="1,1,1,1"/>
            <Setter Property="BorderBrush" Value="#FF3B3B3B"/>
            <Setter Property="HorizontalAlignment" Value="Left"/>
            <Setter Property="VerticalAlignment" Value="Top"/>
            <Setter Property="CornerRadius" Value="4"/>
            <!--
            <Setter Property="Effect">
                <Setter.Value>
                    <DropShadowEffect Direction="-90" ShadowDepth="0" Color="#FFF0F0F0" BlurRadius="3"/>
                </Setter.Value>
            </Setter>
            -->
        </Style>

        <Style TargetType="Border"
           x:Key="category_button_border">
            <Setter Property="Height" Value="28"/>
            <Setter Property="Margin" Value="0,0,10,0"/>
            <Setter Property="BorderThickness" Value="1,1,1,1"/>
            <Setter Property="HorizontalAlignment" Value="Left"/>
            <Setter Property="VerticalAlignment" Value="center"/>
            <Setter Property="CornerRadius" Value="14"/>
            <!--
            <Setter Property="Effect">
                <Setter.Value>
                    <DropShadowEffect Direction="-90" ShadowDepth="0" Color="#FFF0F0F0" BlurRadius="3"/>
                </Setter.Value>
            </Setter>
            -->
        </Style>

        <Style TargetType="Button" x:Key="category_button">
            <Setter Property="Height" Value=" 28"/>
            <Setter Property="Foreground" Value=" White"/>
            <Setter Property="FontSize" Value="14"/>
            <Setter Property="HorizontalAlignment" Value="Left"/>
            <Setter Property="VerticalAlignment" Value="center"/>
            <Setter Property="Background" Value="#FF353535"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">

                        <Border x:Name="border" CornerRadius="14" BorderBrush="{x:Null}" BorderThickness="0" Background ="#FF353535" Margin ="0,0,10,0" Padding = "12,0,12,2">
                            <ContentPresenter HorizontalAlignment="Center"
                                          VerticalAlignment="Center">
                            </ContentPresenter>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsPressed" Value="True">
                                <Setter Property="Background" TargetName="border"  Value="#FF444444">
                                </Setter>
                            </Trigger>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Background" TargetName="border" Value="#FF444444"/>
                            </Trigger>

                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <!--
            <Setter Property="Effect">
                <Setter.Value>
                    <DropShadowEffect Direction="-90" ShadowDepth="0" Color="#FFF0F0F0" BlurRadius="3"/>
                </Setter.Value>
            </Setter>
            -->
        </Style>

